<!-- Contact -->
<section id="contact">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase"><%= gettext "Contact Us" %></h2>
        <h3 class="section-subheading text-muted"><%= gettext "Let’s make products your clients want!" %>"</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <form id="contactForm" name="sentMessage" novalidate="novalidate">
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <input class="form-control" id="name" type="text" placeholder="<%= gettext ~s(Your Name *) %>" required="required" data-validation-required-message="<%= gettext ~s(Please enter your name.) %>">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input class="form-control" id="email" type="email" placeholder="<%= gettext ~s(Your Email *) %>" required="required" data-validation-required-message="<%= gettext ~s(Please enter your email address.) %>">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input class="form-control" id="phone" type="tel" placeholder="<%= gettext ~s(Your Phone *) %>" required="required" data-validation-required-message="<%= gettext ~s(Please enter your phone number.) %>">
                <p class="help-block text-danger"></p>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <textarea class="form-control" id="message" placeholder="<%= gettext ~s(Your Message *) %>" required="required" data-validation-required-message="<%= gettext ~s(Please enter a message.) %>"></textarea>
                <p class="help-block text-danger"></p>
              </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12 text-center">
              <div id="success"></div>
              <button id="sendMessageButton" class="btn btn-primary btn-xl text-uppercase" type="submit"><%= gettext "Send Message" %></button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</section>

